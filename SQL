SELECT DISTINCT LAUNCH_SITE FROM SPACETABL ORDER BY 1;

SELECT * FROM SPACETABL WHERE LAUNCH_SITE LIKE 'CCA%' LIMIT 5;

SELECT SUM(PAYLOAD_MASS__KG_) AS TOTAL_PAYLOAD FROM SPACETABL WHERE PAYLOAD  LIKE '%CRS%';

SELECT AVG(PAYLOAD_MASS__KG_) AS AVG_PAYLOAD FROM SPACETABL WHERE BOOSTER_VERSION = 'F9 v1.1';

SELECT MIN(DATE) AS FIRST_SUCCESS_GP FROM SPACETABL WHERE LANDING__OUTCOME = 'Success (ground pad)';

SELECT DISTINCT BOOSTER_VERSION FROM SPACETABL WHERE PAYLOAD_MASS__KG_ BETWEEN 4000 AND 6000 AND LANDING__OUTCOME = 'Success (drone ship)';

SELECT MISSION_OUTCOME, COUNT(*) AS QTY FROM SPACETABL GROUP BY MISSION_OUTCOME ORDER BY MISSION_OUTCOME;

SELECT DISTINCT BOOSTER_VERSION FROM SPACETABL WHERE PAYLOAD_MASS__KG_ = (SELECT MAX(PAYLOAD_MASS__KG_) FROM SPACEXTBL) ORDER BY BOOSTER_VERSION;

SELECT BOOSTER_VERSION, LAUNCH_SITE FROM SPACETABL WHERE LANDING__OUTCOME = 'Failure (drone ship)' AND DATE_PART('YEAR', DATE) = 2015;

SELECT LANDING__OUTCOME, COUNT(*) AS QTY FROM SPACETABL WHERE DATE BETWEEN '2010-06-04' AND '2017-03-20' GROUP BY LANDING__OUTCOME ORDER BY QTY DESC
